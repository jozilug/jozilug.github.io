---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>www.jozilug.co.za</title>
 <link href="http://www.jozilug.co.za/atom.xml" rel="self"/>
 <link href="http://www.jozilug.co.za/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.jozilug.co.za/</id>
 <author>
   <name>Daniel Fowler</name>
   <email>me@danfowler.net</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.jozilug.co.za{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.jozilug.co.za{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
